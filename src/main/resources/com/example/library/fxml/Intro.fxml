   <children>
      <MediaView fx:id="mediaView" fitHeight="360.0" fitWidth="640.0" />
      <Button fx:id="tryAiNowButton" layoutX="366.0" layoutY="397.0" mnemonicParsing="false" onAction="#handleTryAiNow" prefHeight="50.0" prefWidth="135.0" text="Try AI now" />
      <Button fx:id="returnButton" layoutX="140.0" layoutY="397.0" mnemonicParsing="false" onAction="#handleReturnToLib" prefHeight="50.0" prefWidth="135.0" text="Return to library" />
   </children>
</AnchorPane>
