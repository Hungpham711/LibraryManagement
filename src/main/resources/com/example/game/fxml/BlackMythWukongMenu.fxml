<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.game.BlackMythWukongMenuController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1100.0" pickOnBounds="true" preserveRatio="false">
         <image>
            <Image url="@../assets/BMWmenu.jpg" />
         </image>
      </ImageView>
      <Button fx:id="openButton" layoutX="163.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleOpenButton" prefHeight="45.0" prefWidth="121.0" text="Play" />
      <Button fx:id="goBackButton" layoutX="163.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleReturnToLib" prefHeight="45.0" prefWidth="121.0" text="Return to Library" />
   </children>
</AnchorPane>
